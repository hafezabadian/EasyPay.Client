{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Chrome Launch",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200/",
            "webRoot": "${workspaceFolder}/EasyPay.Admin"
        },
        {
            "name": "Firefox Launch",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/EasyPay.Admin"
        },
        {
            "name": "Edge Launch",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/EasyPay.Admin"
        },
        {
            "type": "vscode-edge-devtools.debug",
            "request": "launch",
            "name": "Edge DevTools",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/EasyPay.Admin"
        }
    ]
}